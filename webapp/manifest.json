{
	"sap.app": {
		"_version": "1.3.0",
		"id": "ibm.fin.ar",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{keyword1}}"]
		},
		"ach": "FIN-CORE-AR"
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.66.10",
			"libs": {
				"sap.m": {
					"minVersion": "1.66.10"
				},
				"sap.ui.layout": {
					"minVersion": "1.66.10"
				},
				"sap.ui.core": {
					"minVersion": "1.66.10"
				}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		},
		"resources": {
			"css": [{
				"uri": "css/mystyle.css",
				"id": ""
			}]
		},
		"rootView": {
			
			"viewName": "ibm.fin.ar.view.App",
			"type": "XML",
			"id": "idApp"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ibm.fin.ar.view",
				 "viewType": "XML",
				 "controlId": "myApp"
				
			},
			"routes": [
				{
					"pattern": "",
					"name": "anubhav",
					"target":[ "donald","trumph"]
				},
				{
					"pattern": "fruitDetail/{index}",
					"name": "oreroy",
					"target":[ "donald","putin"]
				}
				
				],
			"targets": {
				
				"donald":{
					"viewName": "View1",
					"viewID": "idView1",
					"controlAggregation": "masterPages"
				},
				"trumph":{
				"viewName": "Empty",
					"viewID": "idEmpty",
					"controlAggregation": "detailPages"
				},
				"putin":{
				"viewName": "View2",
					"viewID": "idView2",
					"controlAggregation": "detailPages"
				}
			}

		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal", "sap_belize"]
	},
	"_version": "1.9.0"
}